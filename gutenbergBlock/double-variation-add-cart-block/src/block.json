{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/double-variation-add-cart-block",
	"version": "0.1.0",
	"title": "Double variation add cart block",
	"category": "widgets",
	"description": "Display a double add to cart button for a product with a price and variation. The price is the same in all variations.",
	"example": {},
	"attributes": {
		"productName": {
			"type": "string"
		},
		"productPrice": {
			"type": "string"
		},
		"variation1Number": {
			"type": "string"
		},
		"variation1Name": {
			"type": "string"
		},
		"variations1": {
			"type": "array"
		},
		"variation2Number": {
			"type": "string"
		},
		"variation2Name": {
			"type": "string"
		},
		"variations2": {
			"type": "array"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"textdomain": "wp-ultra-simple-paypal-shopping-cart",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}
